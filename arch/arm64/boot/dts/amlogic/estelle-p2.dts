	bl: backlight@2c {
		compatible = "ti,lp8556";
		reg = <0x2c>;
		bl-name = "lcd-bl";
		// brightness register control only backlight enabled
		dev-ctrl = /bits/ 8 <0x05>;
		// 1/2 brightness
		init-brt = /bits/ 16 <0x800>;
		// CURRENT LSB
		rom_a0h {
			rom-addr = /bits/ 8 <0xa0>;
			rom-val = /bits/ 8 <0xff>;
		};
		// Standby disabled, 20mA MAX_CURRENT, CURRENT scaled down by 2
		rom_a1h {
			rom-addr = /bits/ 8 <0xa1>;
			rom-val = /bits/ 8 <0x37>;
		};
		// Enable undervoltage protection at  2.5 V, "disable"
		// backlight (i2c only), disable set resistors
		rom_a2h {
			rom-addr = /bits/ 8 <0xa2>;
			rom-val = /bits/ 8 <0x20>;
		};
		// 9.6kHz PWM rate, 3 phase drivers
		rom_a5h {
			rom-addr = /bits/ 8 <0xa5>;
			rom-val = /bits/ 8 <0x54>;
		};
	};

